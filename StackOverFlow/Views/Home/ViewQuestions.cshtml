@model List<StackOverFlow.ViewModels.QuestionViewModel>
@{
    ViewBag.Title = "ViewQuestions";
}

<div class="row">
    <div class="col-sm-12">
        <table class="table">
            @foreach (var item in Model)
            {
                Html.RenderPartial("Question", item);
            }
        </table>
        <br />
    </div>
</div>

<script>
    $("a[href='/home/categories']").attr("id", "active");
</script>