@model List<StackOverFlow.ViewModels.QuestionViewModel>
@{ 
    ViewBag.Title = "Home";
}
<div class="row">
    <div class="col-sm-12">
        <div class="jumbotron">
            <h2>Learn, Shared, Build!</h2>
            <h5>Each month, over 50 million developers come to Unstuck to learn, share their knowledge, and build their careers</h5>
             <p class="lead">Join the world's largest developer community</p>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <table class="table">
                @foreach(var item in Model)
                {
                    Html.RenderPartial("Question", item);
                }
            </table>
            @if (Session["CurrentUserName"] != null)
            {
                <a href="/questions/create" class="btn btn-primary">
                    Add Question
                </a>
            }
        </div>
    </div>
</div>
<script>
    $("a[href='/home/index']").attr("id", "active");
</script>